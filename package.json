{
  "name": "playwright-testing-demo",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "e2e": "NODE_OPTIONS='--enable-source-maps' playwright test --config=playwright.config.ts",
    "e2e:cache": "NODE_OPTIONS='--enable-source-maps' cross-env MIDSCENE_CACHE=true playwright test --config=playwright.config.ts",
    "e2e:ui": "NODE_OPTIONS='--enable-source-maps' playwright test --config=playwright.config.ts --ui",
    "e2e:ui:cache": "NODE_OPTIONS='--enable-source-maps' cross-env MIDSCENE_CACHE=true playwright test --config=playwright.config.ts --ui",
    "postinstall": "pnpm exec playwright install"
  },
  "devDependencies": {
    "@midscene/web": "latest",
    "@playwright/test": "1.52.0",
    "@types/jest": "~29.5.14",
    "@types/node": "~22.7.9",
    "cross-env": "7.0.3",
    "dotenv": "16.4.5",
    "eslint-plugin-prettier": "~5.2.1",
    "rimraf": "~6.0.1",
    "tsx": "4.20.3",
    "typescript": "~5.6.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "console-table-printer": "^2.15.0"
  }
}
