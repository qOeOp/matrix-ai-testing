{
  "name": "matrix-ai-testing",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "e2e": "NODE_OPTIONS='--enable-source-maps' playwright test --config=playwright.config.ts",
    "e2e:cache": "NODE_OPTIONS='--enable-source-maps' cross-env MIDSCENE_CACHE=true playwright test --config=playwright.config.ts",
    "e2e:ui": "NODE_OPTIONS='--enable-source-maps' playwright test --config=playwright.config.ts --ui",
    "e2e:ui:cache": "NODE_OPTIONS='--enable-source-maps' cross-env MIDSCENE_CACHE=true playwright test --config=playwright.config.ts --ui",
    "server": "tsx e2e/lark/server.ts",
    "server:dev": "tsx watch e2e/lark/server.ts",
    "postinstall": "pnpm exec playwright install chromium"
  },
  "dependencies": {
    "@larksuiteoapi/node-sdk": "^1.55.0",
    "@midscene/web": "^0.30.11-beta-20251218071621.0",
    "console-table-printer": "^2.15.0",
    "dotenv": "^16.4.5",
    "openai": "^4.77.3",
    "playwright": "^1.52.0",
    "reflect-metadata": "^0.2.2",
    "uuid": "^13.0.0",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.7.9",
    "@types/uuid": "^10.0.0",
    "cross-env": "^7.0.3",
    "eslint-plugin-prettier": "^5.2.1",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.3",
    "typescript": "^5.6.3"
  }
}
